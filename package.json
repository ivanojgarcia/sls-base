{
  "name": "a-go-ms-examens",
  "version": "1.0.0",
  "description": "Microservices for the management of examination records",
  "main": "handler.js",
  "scripts": {
    "test-dev": "STAGE=test jest",
    "test": "echo \"Error: no test specified\" && exit 1",
    "offline": "sls offline start --httpPort 3004 --lambdaPort 3006"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "html"
    ],
    "coverageDirectory": "<rootDir>/coverage/",
    "collectCoverageFrom": [
      "**/app/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 10,
        "lines": 10,
        "statements": 10
      }
    }
  },
  "author": "Ivano J. Garc√≠a",
  "license": "ISC",
  "devDependencies": {
    "jest": "^26.2.2",
    "serverless-offline": "^6.5.0",
    "serverless-plugin-optimize": "^4.1.4-rc.1"
  },
  "dependencies": {
    "aws-sdk": "^2.726.0",
    "dotenv": "^8.2.0",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "node-fetch": "^2.6.0",
    "serverless-pseudo-parameters": "^2.5.0",
    "uuid": "^8.3.0"
  }
}
